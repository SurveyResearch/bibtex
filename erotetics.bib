@article{schaffer_knowing_2007,
	title = {Knowing the Answer},
	volume = {75},
	%issn = {1933-1592},
	%url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1933-1592.2007.00081.x/abstract},
	doi = {10.1111/j.1933-1592.2007.00081.x},
	abstract = {How should one understand knowledge-wh ascriptions? That is, how should one understand claims such as “I know where the car is parked,” which feature an interrogative complement? The received view is that knowledge-wh reduces to knowledge that p, where p happens to be the answer to the question Q denoted by the wh-clause. I will argue that knowledge-wh includes the question—to know-wh is to know that p, as the answer to Q. I will then argue that knowledge-that includes a contextually implicit question. I will conclude that knowledge is a question-relative state. Knowing is knowing the answer, and whether one knows the answer depends (in part) on the question.},
