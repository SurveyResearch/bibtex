@article{aloni_alternative_2010,
	title = {Alternative Questions and Knowledge Attributions},
	volume = {60},
	url = {http://www.jstor.org/stable/40468275},
	abstract = {We discuss the 'problem of convergent knowledge', an argument presented by J. Schaffer in favour of contextualism about knowledge attributions, and against the idea that knowledge-wh can be simply reduced to knowledge of the proposition answering the question. Schaffer's argument centrally involves alternative questions of the form 'whether A or B'. We propose an analysis of these on which the problem of convergent knowledge does not arise. While alternative questions can contextually restrict the possibilities relevant for knowledge attributions, what Schaffer's puzzle reveab is a pragmatic ambiguity in what 'knowing the answer' means: in his problematic cases, the subject knows only a partial answer to the question. This partial knowledge can be counted as adequate only on externalist grounds.},
	number = {238},
	journal = {The Philosophical Quarterly},
	author = {Aloni, Maria and Égré, Paul},
	month = jan,
	year = {2010},
	keywords = {Questions},
	pages = {1--27}
},

